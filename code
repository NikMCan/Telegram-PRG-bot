       //if(sheetLog.getRange(rowLog,4).getValue() != cbText) { //–ø–æ–≤—Ç–æ—Ä–Ω–æ–µ –Ω–∞–∂–∞—Ç–∏–µ –Ω–∞ —á–ª–µ–Ω–∞
         rowMembers = cbText.slice(2);
         vklad(4);}
         //else sendmes(1,sheetMembers.getRange(cbText.slice(2),3).getValue() + '\n' + sheetMembers.getRange(cbText.slice(2),13).getValue() + ' –£–ò–ö ‚Ññ' + sheetMembers.getRange(cbText.slice(2),4).getValue() + '. ' + sheetMembers.getRange(cbText.slice(2),10).getValue(),key8,fromId);
