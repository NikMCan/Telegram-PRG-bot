    if(cbText.slice(1,2) == 'c') rez(1*cbText.slice(2,3)); //–æ—Ç–∫—Ä—ã–≤–∞–µ–º –º–µ–Ω—é –≤—ã–±–æ—Ä–∞ –ø–æ–ª—è –¥–ª—è –∫–æ–ª–æ–Ω–∫–∏
    if(cbText.slice(1,2) == 'v') { //–≤—ã–±–∏—Ä–∞–µ–º –ø–æ–ª–µ –¥–ª—è –∫–æ–ª–æ–Ω–∫–∏
      if(status==0) var mcb = sheetTov.getRange(rowTov,6).getValue().split(' ');
        else var mcb = sheetMembers.getRange(rowMembers,27).getValue().split(' ');
      mcb[1*cbText.slice(cbText.indexOf('c')+1)] = 1*cbText.slice(2,cbText.indexOf('c'));
      if(status==0) sheetTov.getRange(rowTov,6).setValue(mcb[0]+' '+mcb[1]+' '+mcb[2]+' '+mcb[3]+' '+mcb[4]+' '+mcb[5]+' '+mcb[6]);
        else sheetMembers.getRange(rowMembers,27).setValue(mcb[0]+' '+mcb[1]+' '+mcb[2]+' '+mcb[3]+' '+mcb[4]+' '+mcb[5]+' '+mcb[6]);
      delmes(fromId,messageId); 
      rez(-1);
    }
    if(cbText.slice(1,2) == 'm') { //–ø–æ–¥—Ä–æ–±–Ω–æ—Å—Ç–∏ –ø—Ä–∏ –Ω–∞–∂–∞—Ç–∏–∏ –Ω–∞ —Ü–∏—Ñ—Ä—É
