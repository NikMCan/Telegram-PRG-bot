  var uik = sheetMembers.getRange(rowMembers,4).getValue(); 
  rBut.push([{text:uik,callback_data:'12' + uik}]);  
  for(var i=0; i<partStatus; i++) {
      med = sheetMembers.getRange(rowMembers,19).getValue();    
      if(med!=0) rBut[rBut.length-1].push({text:med,callback_data:'13'+rowMembers}); 
        else rBut[rBut.length-1].push({text:' ',callback_data:'14' + '\n' + uik});
      rowMembers = rowMembers + 1;     
  }
}
