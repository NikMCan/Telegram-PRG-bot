function document(docId,capt,key,fromId){
  var data = {
    method: "post",
    payload: {method: "sendDocument",chat_id: String(fromId),document: docId,caption: capt,reply_markup: JSON.stringify(key)}
  }
  UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
}

function photo(fromId){ //—Ñ—É–Ω–∫—Ü–∏—è –æ—Ç—Å—ã–ª–∞–µ—Ç —Ñ–æ—Ç–æ. –ù–µ –∏—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è, —à–ª–µ–º –∞–ª—å–±–æ–º.
  var data = {
    method: "post",
    payload: {method: "sendPhoto",chat_id: String(fromId),photo: 'AgADAgADe6oxG6WLKUrLDqFzIu5ZrDb2tw8ABAEAAwIAA3gAA8nLAQABFgQ',reply_markup: JSON.stringify(key)}
  }
   UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
}

function delmes(fromId,messageId){ //—É–¥–∞–ª—è–µ–º –Ω–µ–Ω—É–∂–Ω—ã–µ —Å–æ–æ–±—â–µ–Ω–∏—è
  var data = {
    method: "post",
    payload: {method: "deleteMessage",chat_id: String(fromId),message_id: messageId}
  }
   UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
}


function sendmes(lg,text,key,chId){ //–ø–æ—Å—ã–ª–∞–µ–º –Ω–æ–≤–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
  var data = {
    method: "post",
    payload: {method: "sendMessage",chat_id: String(chId),text: text,parse_mode: "HTML",reply_markup: JSON.stringify(key)}
  }
   var ret = UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
   if(lg==1) log(fromId,fullName,name,rowMembers,cbText,botUpdate);
   ret =JSON.parse(ret);
  //sheetMembers.getRange(1,3).setValue(ret.result.message_id);
  return ret.result.message_id;
}


function newmessage(lg,text,key,fromId,messageId){ //–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –º–µ–Ω—é
  var data = {
  method: "post",
  payload: {method: "editMessageText",chat_id: String(fromId),message_id: messageId,text: text,parse_mode: "HTML",reply_markup: JSON.stringify(key)}
  } 
  UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
  if(lg == 1) log(fromId,fullName,name,rowMembers,cbText,botUpdate);
}

function answercb(text,cbId){//–≤—Å–ø–ª—ã–≤–∞—é—â–µ–µ –º–µ–Ω—é –≤ —Ä–µ–∑—É–ª—Ç–∞—Ç–µ –Ω–∞–∂–∞—Ç–∏—è –Ω–∞ –∫–Ω–æ–ø–∫–∏
    var data = {
      method: "post",
      payload: {method: "answerCallbackQuery",callback_query_id: String(cbId),show_alert: true,text: text}
     }        
      UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
      log(fromId,fullName,name,rowMembers,cbText,botUpdate);
}

function log(Id,fullName,name,rowMembers,text,msg){ //—Ñ—É–Ω–∫—Ü–∏—è –∑–∞–ø–∏—Å–∏ –ª–æ–≥–∞
  var rowLog = sheetLog.getRange(3, 3).getValue();
  var rowLogError = sheetLogError.getRange(3, 3).getValue();  
  //var dateTime = new Date();
  //var key5 = {keyboard:[['üîá –í—ã–∫–ª—é—á–∏—Ç—å —Ç—Ä–∞–Ω—Å–ª—è—Ü–∏—é –ª–æ–≥–∞']],resize_keyboard: true,one_time_keyboard: true}; //—Å —Ä–µ—Ç—Ä–∞–Ω—Å–ª—è—Ü–∏–µ–π
  
  
  if(status==0){ //–í –ª–æ–≥ –Ω–µ–æ–ø–æ–∑–Ω–∞–Ω–Ω—ã—Ö —Å–æ–æ–±—â–µ–Ω–∏–π      
    sheetLogError.getRange(3, 3).setValue(rowLogError+1);
    sheetLogError.getRange(rowLogError+1,1).setValue(formattedTime);
    sheetLogError.getRange(rowLogError+1,2).setValue(Id);
    sheetLogError.getRange(rowLogError+1,3).setValue(fullName);
    sheetLogError.getRange(rowLogError+1,4).setValue(text);
    sheetLogError.getRange(rowLogError+1,6).setValue(msg);        
    }
    else { //–í –ª–æ–≥ –æ–ø–æ–∑–Ω–∞–Ω–Ω—ã—Ö
      //sheetLog.getRange(rowLog+1,6).setValue(msg);
      //if(sheetLog.getRange(rowLog+1,4).getValue() != sheetLog.getRange(rowLog,4).getValue()){
        sheetLog.getRange(3, 3).setValue(rowLog+1); //—Ä–∞—Å—Ç–µ—Ç –Ω–æ–º–µ—Ä —Å—Ç—Ä–æ–∫–∏
        sheetLog.getRange(rowLog+1,1).setValue(formattedTime);
        sheetLog.getRange(rowLog+1,2).setValue(Id);
        sheetLog.getRange(rowLog+1,3).setValue(name);
        sheetLog.getRange(rowLog+1,4).setValue(text);
      //}
   }
}




function rasstanovka(text){ //–º–µ–Ω—é —Ä–∞—Å—Å—Ç–∞–Ω–æ–≤–∫–∞
var rBut = [[{text:'üìù Info',url:'https://telegra.ph/Az-esm-07-17'}]]; //17Info
rBut.push([{text:'‚¨Ö –ù–∞–∑–∞–¥',callback_data:'1–ù–∞–∑–∞–¥'}]);

