var key = {inline_keyboard:rBut};
text = text +t[j];
if(j==0) newmessage(0,text,key,fromId,messageId);
  else sendmes(1,text,key,fromId);
j=j+1;
u.shift();
rBut.length = 0;
text='';
}
}

