var o =[0,'–°–ó–ê–û','–°–ê–û','–°–í–ê–û','–ó–ê–û','–¶–ê–û','–í–ê–û','–Æ–ó–ê–û','–Æ–ê–û','–Æ–í–ê–û','–ó–µ–ª–ê–û','–ù–ê–û','–¢–ê–û'];
var vdBut = [[{text:'üìù Info',url:'https://telegra.ph/Voznagrazhdenie---analiz-08-12'}],[{text:'‚¨Ö –ù–∞–∑–∞–¥',callback_data:'91–ù–∞–∑–∞–¥'+o[okr]}]];  
var rowUik = 4; var uik = sheetVideo[okr][raion].getRange(rowUik,1).getValue();
while(uik!=0){
  vdBut.push([{text:sheetVideo[okr][raion].getRange(rowUik,15).getValue(),callback_data:'93'+okr+'R'+raion+'U'+rowUik}]); rowUik = rowUik +1;
  for(var i=0; i<2; i++) {if(uik!=0) {vdBut[vdBut.length-1].push({text:sheetVideo[okr][raion].getRange(rowUik,15).getValue(),callback_data:'93'+okr+'R'+raion+'U'+rowUik}); rowUik = rowUik +1; uik = sheetVideo[okr][raion].getRange(rowUik,1).getValue();}}
