    method: "post",
    payload: {method: "sendLocation",chat_id: String(chId),latitude: 55.657535,longitude: 37.406531,reply_markup: JSON.stringify(key)}
  }
   UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);  
}

