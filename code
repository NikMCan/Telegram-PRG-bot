    if(cbText.slice(0,2) == '15') answercb('–ß–ª–µ–Ω—ã, –æ—Ñ–æ—Ä–º–ª–µ–Ω–Ω—ã–µ –æ—Ç ' + cbText.slice(2),cbId);
    if(cbText.slice(0,2) == '17')  answercb(text12,cbId);    
       
  }
  }  
  //===================================================================================================

  //–ü—Ä–æ–≤–µ—Ä—è–µ–º —è–≤–ª—è–µ—Ç—Å—è –ª–∏ –∞–ø–¥–µ–π—Ç —Å–æ–æ–±—â–µ–Ω–∏–µ–º
  if (update.hasOwnProperty('message')) {
    messageId = update.message.message_id;
    var mesText = update.message.text;
    log(update.message.chat.id, update.message.chat.first_name + '  ' + update.message.chat.last_name, name, rowMembers,e.postData.contents, update.message);
 
    if(update.message.hasOwnProperty('photo')) { //–ø—Ä–æ–≤–µ—Ä—è–µ—Ç —è–≤–ª—è–µ—Ç—Å—è –ª–∏ —Å–æ–æ–±—â–µ–Ω–∏–µ —Ñ–æ—Ç–∫–æ–π –∏ —Ç –¥
      if(e.postData.contents.indexOf('caption')>0) var cap = e.postData.contents.slice(e.postData.contents.lastIndexOf('caption')+11,e.postData.contents.lastIndexOf('\"'));
      if(e.postData.contents.indexOf('media_group_id')>0) var mgId = e.postData.contents.slice(e.postData.contents.lastIndexOf('media_group_id')+17,e.postData.contents.lastIndexOf('photo')-3);
      var phId = e.postData.contents.slice(e.postData.contents.lastIndexOf('file_id')+10,e.postData.contents.lastIndexOf('file_size')-3);            
      var c = [['u041e',20],['u0413',21],['u041f',22],['u0424',23]];
      log(update.message.chat.id, update.message.chat.first_name + '  ' + update.message.chat.last_name, name, rowMembers,cap+' '+mgId+' '+messageId+' '+phId, update.message);

      for(var i=0; i<4; i++){        
        if(cap==c[i][0]){          
          if(e.postData.contents.indexOf('media_group_id')>0) {sheetDoki.getRange(rowMembers,10*(i+1)-9+1*String(messageId).slice(-1)).setValue(mgId+' '+messageId+' '+phId); break;}
            else {sheetDoki.getRange(rowMembers, 10*(i+1)-9, 1, 10).clear(); sheetDoki.getRange(rowMembers, 10*(i+1)-9).setValue(1+' '+1+' '+phId);  break;}
      }      
    }
    }
      else log(update.message.chat.id, update.message.chat.first_name + '  ' + update.message.chat.last_name, name, rowMembers,update.message.text, update.message);
   
    if(mesText == '/start' && status!=0) sendmes(1,text0,key2,update.message.chat.id);
    if(mesText == '/start' && status==0) sendmes(1,text1,key2,update.message.chat.id);
    if(mesText == '–û –£–¥–∞–ª–∏—Ç—å') sheetDoki.getRange(rowMembers, 1, 1, 10).clear();
    if(mesText == '–ì –£–¥–∞–ª–∏—Ç—å') sheetDoki.getRange(rowMembers, 11, 1, 10).clear();
    if(mesText == '–ü –£–¥–∞–ª–∏—Ç—å') sheetDoki.getRange(rowMembers, 21, 1, 10).clear();
    if(mesText == '–§ –£–¥–∞–ª–∏—Ç—å') sheetDoki.getRange(rowMembers, 31, 1, 10).clear();
    
    if(mesText.slice(0,1)=='V' && 1*mesText.slice(1,2)>0 && 1*mesText.slice(1,mesText.indexOf(','))<19 ) { //–æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º –≤–≤–æ–¥ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è 19 —ç—Ç–æ —á–∏—Å–ª–æ —Å—Ç—Ä–æ–∫ –ø–ª—é—Å –æ–¥–∏–Ω - –º–µ–Ω—è–π—Ç–µ –≤ –¥–≤—É—Ö –º–µ—Å—Ç–∞—Ö
         sheetRez.getRange(1,1).setValue(sheetMembers.getRange(rowMembers,4).getValue());
         var rowZ = sheetRez.getRange(1,2).getValue();
         var v = mesText.split(',');
      if(v.length-2+1*mesText.slice(1,mesText.indexOf(','))<19) {
      for(i=1; i<v.length; i++) {        
          sheetRez.getRange(rowZ,i+2+1*mesText.slice(1,mesText.indexOf(','))).setValue(1*v[i]);      
          delmes(fromId,messageId);
        sheetRez.getRange(rowZ,1).setValue(formattedTime);}}
        else sendmes(1,'–û—à–∏–±–∫–∞ –≤ –∫–æ–ª–∏—á–µ—Å—Ç–≤–µ –≤–≤–æ–¥–∏–º—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π',key8,fromId);      
    }
    
    
     if(mesText!='/start' && mesText!='–û –£–¥–∞–ª–∏—Ç—å' && mesText!='–ì –£–¥–∞–ª–∏—Ç—å' && mesText!='–ü –£–¥–∞–ª–∏—Ç—å' && mesText!='–° –£–¥–∞–ª–∏—Ç—å' && mesText.slice(0,1)!='V') sendmes(1,'–Ø –≤—Å–µ–≥–æ –ª–∏—à—å —Ä–æ–±–æ—Ç! –ò—Å–∫—É—Å—Ç–≤–µ–Ω–Ω—ã–º –∏–Ω—Ç–µ–ª–ª–µ–∫—Ç–æ–º –Ω–µ –æ–±–ª–∞–¥–∞—é - —á—Ç–æ –≤—ã –Ω–∞–ø–∏—Å–∞–ª–∏ –Ω–µ –ø–æ–Ω–∏–º–∞—é. –ü—Ä–æ—Å—Ç–æ –∂–º–∏—Ç–µ –Ω–∞ –∫–Ω–æ–ø–∫–∏ –∏ –Ω–µ –ø—É–≥–∞–π—Ç–µ –º–µ–Ω—è)))',key8,fromId);
 
  }
    }
    
