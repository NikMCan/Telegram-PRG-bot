function aut(id){ //–º–æ–¥—É–ª—å –∞–≤—Ç–æ—Ä–∏–∑–∞—Ü–∏–∏ —Ç–µ–ª–µ–≥—Ä–∞–º ID –∏ —Å—Ç—Ä–æ–∫–∞ –≤ —Ç–∞–±–ª–∏—Ü–µ. –ù–µ –∫—Ä–∞—Å–∏–≤–æ, –∑–∞—Ç–æ –æ—á–µ–Ω—å –±—ã—Å—Ç—Ä–æ.
  switch (id){ 
    case 33333333:  rowMembers = 133; break;//–°–∏–∑–æ–≤ –ù–∏–∫–æ–ª–∞–π –ò–≥–æ—Ä–µ–≤–∏—á

    //default: sendmes(1,'–í—ã –Ω–µ –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞–Ω—ã, –æ–±—Ä–∞—Ç–∏—Ç–µ—Å—å –∫ –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä—É.',{remove_keyboard: true},fromId);
      
    default: {status = 0; rowMembers = 9;
             sheetTov.getRange(1,1).setValue(id);
             rowTov=sheetTov.getRange(1,2).getValue();
              if(rowTov==0) {rowTov = sheetTov.getRange(1,3).getValue();
                            sheetTov.getRange(rowTov,2).setValue(id);
                            sheetTov.getRange(rowTov,3).setValue(update.message.chat.first_name + '  ' + update.message.chat.last_name);
                            sheetTov.getRange(1,3).setValue(rowTov+1);}
             }
  }
}
