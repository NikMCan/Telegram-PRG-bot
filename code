}

function rez(nStr){ //–≤—ã–≤–æ–¥–∏–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è
var cb=sheetMembers.getRange(rowMembers,27).getValue(); //–∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞ —Å—Ç–æ–ª–±—Ü–æ–≤ –†–µ–∑—É–ª—å—Ç–∞—Ç–∞
if(status==0) cb=sheetTov.getRange(rowTov,6).getValue();
var mcb = cb.split(' ');
for(var i=0; i<mcb.length-1; i++) mcb[i]=1*mcb[i];  //–ø–µ—Ä–µ–≤–æ–¥–∏–º –∏–∑ —Å—Ç—Ä–æ–∫–∏ –≤ —Ü–∏—Ñ—Ä—É
var col = [], mB = [], But = [];
var sort = ['','','','','','','']; //–º–∞—Å—Å–∏–≤ –¥–ª—è –∑–Ω–∞—á–∫–∞ —Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∏ —Å—Ç–æ–ª–±—Ü–∞
if(mcb[6]=='1') sort[mcb[6]] = 'üî∫'; //–°–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ –Ω–æ–º–µ—Ä–∞–º —É–∏–∫–æ–≤ - –ø–æ –≤–æ–∑—Ä–∞—Å—Ç–∞–Ω–∏—é, –æ—Å—Ç–∞–ª—å–Ω—ã–µ - –ø–æ —É–±—ã–≤–∞–Ω–∏—é
  else sort[mcb[6]] = 'üîª';
var uik = []; //–º–∞—Å—Å–∏–≤ —Å –∫–æ–ª–∏—á–µ—Å—Ç–≤–æ–º –∫–æ–º–∏—Å—Å–∏–π –≤ –∫–∞–∂–¥–æ–º –ø–æ—Å—Ç–µ. –ü–æ–º–Ω–∏–º –º–∞–∫–∏–º—É–º –≤–æ–∑–º–æ–∂–Ω–æ 100 –∫–Ω–æ–ø–æ–∫ –Ω–∞ –ø–æ—Å—Ç
for (var i=0; i<u.length-1; i++) uik.push((u[i+1]-u[i])/4);
var sum = 0;
var text = ['–¢–∞–±–ª–∏—Ü–∞ –∏—Ç–æ–≥–æ–≤ –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è. –û–±–Ω–æ–≤–ª—è–µ—Ç—Å—è –ø–æ –º–µ—Ä–µ –ø–æ—Å—Ç—É–ø–ª–µ–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö','–í—Ç–æ—Ä–æ–π –±–ª–æ–∫','–¢—Ä–µ—Ç–∏–π –±–ª–æ–∫','–ß–µ—Ç–≤–µ—Ä—Ç—ã–π –±–ª–æ–∫','–ü—è—Ç—ã–π –±–ª–æ–∫','–í—ã–±–µ—Ä–∏—Ç–µ —Å—Ç–æ–ª–±–µ—Ü, —á—Ç–æ –±—É–¥–µ—Ç –≤—ã–≤–µ–¥–µ–Ω –≤ —Ç–∞–±–ª–∏—Ü—É —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –≥–æ–ª–æ—Å–æ–≤–∞–Ω–∏—è.']; 
if(nStr<1){ var But = [[{text:'üìù Info',url:'https://telegra.ph/Itogi-vyborov-08-14'}],[{text:'‚¨Ö –ù–∞–∑–∞–¥',callback_data:'21–ù–∞–∑–∞–¥'}]];
  for(var i=0; i<mcb.length-1; i++) col.push(sheetRez.getRange(2,1*mcb[i]).getValue());
  But.push([{text:sort[1]+col[0],callback_data:'2s1'},{text:col[1],callback_data:'2c1'},{text:col[2],callback_data:'2c2'},{text:col[3],callback_data:'2c3'},{text:col[4],callback_data:'2c4'},{text:col[5],callback_data:'2c5'}]);
  }
  
if(nStr>0) { //–≤—ã–±–∏—Ä–∞–µ–º –ø–æ–ª–µ –¥–ª—è –∫–æ–ª–æ–Ω–∫–∏
  for(var i=0; i<uik.length; i++) delmes(fromId,messageId+i); //—Å—Ç–∏—Ä–∞–µ–º –ø–æ—Å—Ç—ã
  for(var i=0; i<37; i++) col[i]=sheetRez.getRange(1,i+4).getValue(); //—Å—á–∏—Ç—ã–≤–∞–µ–º –Ω–∞–∑–≤–∞–Ω–∏—è –∏–∑ –ø–µ—Ä–≤–æ–π —Å—Ç—Ä–æ–∫–∏ —Ç–∞–±–ª–∏—Ü—ã 
  But.push([{text:col[0],callback_data:'2v4c'+nStr},{text:col[1],callback_data:'2v5c'+nStr},{text:col[2],callback_data:'2v6c'+nStr}],[{text:col[3],callback_data:'2v7c'+nStr},{text:col[4],callback_data:'2v8c'+nStr},{text:col[5],callback_data:'2v9c'+nStr}],[{text:col[6],callback_data:'2v10c'+nStr},{text:col[7],callback_data:'2v11c'+nStr},{text:col[8],callback_data:'2v12c'+nStr}],[{text:col[9],callback_data:'2v13c'+nStr},{text:col[10],callback_data:'2v14c'+nStr},{text:col[11],callback_data:'2v15c'+nStr}],[{text:col[12],callback_data:'2v16c'+nStr}],[{text:col[23],callback_data:'2v27c'+nStr}],
          [{text:col[13],callback_data:'2v17c'+nStr},{text:col[24],callback_data:'2v28c'+nStr}],[{text:col[14],callback_data:'2v18c'+nStr},{text:col[25],callback_data:'2v29c'+nStr}],[{text:col[15],callback_data:'2v19c'+nStr},{text:col[26],callback_data:'2v30c'+nStr}],[{text:col[16],callback_data:'2v20c'+nStr},{text:col[27],callback_data:'2v31c'+nStr}],[{text:col[17],callback_data:'2v21c'+nStr},{text:col[28],callback_data:'2v32c'+nStr}]);
  uik.length=1; uik[0]=0;          }
  else{ //–≤—ã–≤–æ–¥–∏–º —Å–æ–±—Å—Ç–≤–µ–Ω–Ω–æ –º–µ–Ω—é –†–µ–∑—É–ª—å—Ç–∞—Ç
    if(nStr==0){ for(var i=0; i<uik.length; i++) delmes(fromId,messageId+i);} //—Å—Ç–∏—Ä–∞–µ–º –ø–æ—Å—Ç—ã
    //if(nStr==0) sheetMembers.getRange(rowMembers,28).setValue(messageId+2); //–ø–µ—Ä–µ—Ö–æ–¥ —Å –º–Ω–æ–≥–æ–ø–æ—Å—Ç–æ–≤–æ–≥–æ –º–µ–Ω—é
      //else sheetMembers.getRange(rowMembers,28).setValue(messageId+1); // –ø–µ—Ä–µ—Ö–æ–¥ —Å –æ–±—ã—á–Ω–æ–≥–æ
    col = [];
    for(var i=0; i<mcb.length-1; i++) col.push(sheetRez.getRange(4,1*mcb[i]).getValue());
    But.push([{text:sort[0]+col[0],callback_data:'2s0'},{text:sort[2]+col[1],callback_data:'2s2'},{text:sort[3]+col[2],callback_data:'2s3'},{text:sort[4]+col[3],callback_data:'2s4'},{text:sort[5]+col[4],callback_data:'2s5'},{text:sort[6]+col[5],callback_data:'2s6'}]);
    
    if(mcb[6]=='1') sheetRez.getRange(5, 1, 74, 37).sort({column: 1*mcb[mcb[6]-1], ascending: true}); //—Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ —É–∏–∫–∞–º
    else if(mcb[6]=='0') sheetRez.getRange(5, 1, 74, 37).sort({column: 1, ascending: false});
        else   sheetRez.getRange(5, 1, 74, 37).sort({column: 1*mcb[mcb[6]-1], ascending: false}); //—Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ –¥—Ä—É–≥–∏–º
  }
  
for(var j=0; j<uik.length; j++){  
  for(var i=0; i<uik[j]; i++) { 
    for(var k=0; k<6; k++) mB.push(sheetRez.getRange(sum+i+5,mcb[k]).getValue());
    But.push([{text:mB[0],callback_data:'2u'+mB[0]},{text:mB[1],callback_data:'2m'+mcb[1]+'u'+mB[0]+'z'+mB[1]},{text:mB[2],callback_data:'2m'+mcb[2]+'u'+mB[0]+'z'+mB[2]},{text:mB[3],callback_data:'2m'+mcb[3]+'u'+mB[0]+'z'+mB[3]},{text:mB[4],callback_data:'2m'+mcb[4]+'u'+mB[0]+'z'+mB[4]},{text:mB[5],callback_data:'2m'+mcb[5]+'u'+mB[0]+'z'+mB[5]}]);  
    mB.length = 0;
  }
    sum = sum + uik[j]; 

  if(j<uik.length-1) {    
    var key = {inline_keyboard:But};
    var id = sendmes(0,text[j],key,fromId);
    if(nStr<1) sheetMembers.getRange(rowMembers,28).setValue(id);
    But.length=0;}
  else {
    if(nStr<1) But.push([{text:'‚¨Ö –ù–∞–∑–∞–¥',callback_data:'211–ù–∞–∑–∞–¥'}]); 
    else j=5;
    var key = {inline_keyboard:But};
    sendmes(1,text[j],key,fromId);}  
}
  
}

function vvod(){ //–º–µ–Ω—é –≤–≤–æ–¥–∞ —Ä–µ–∑—É–ª—å—Ç–∞—Ç–æ–≤ –≤—ã–±–æ—Ä–æ–≤
sheetRez.getRange(1,1).setValue(sheetMembers.getRange(rowMembers,4).getValue());  //–ø–∏—à–µ–º –Ω–æ–º–µ—Ä –∫–æ–º–∏—Å—Å–∏–∏
var rowZ = sheetRez.getRange(1,2).getValue();  //–ø–æ–ª—É—á–∞–µ–º —Å—Ç—Ä–æ–∫—É
var col = [], zn = [], text = '–£–ò–ö ‚Ññ'+sheetRez.getRange(rowZ,3).getValue();
for(var i=4; i<38; i++) {
  var z = sheetRez.getRange(3,i).getValue();
  if(z!=0){ 
    col.push(z); 
    zn.push(sheetRez.getRange(rowZ,i).getValue());}
    else break;
    }
for(var i=0; i<zn.length; i++) text = text +'\n'+'<b>'+col[i]+'</b>'+'\n'+zn[i];
var key = {inline_keyboard:[[{text:'‚ôª –û–±–Ω–æ–≤–∏—Ç—å',callback_data:'2–û–±–Ω–æ–≤–∏—Ç—å'},{text:'üìù Info',url:'https://telegra.ph/Vvod-rezultatov-vyborov-07-22'}],[{text:'‚¨Ö –ù–∞–∑–∞–¥',callback_data:'2–ó–∞–≥—Ä—É–∑–∫–∞'}]]};  
newmessage(1,text,key,fromId,messageId);
}

function obnovlenie(){ //–∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –æ–±–Ω–æ–≤–ª—è–µ–º –º–µ–Ω—é —Ä–µ–∑—É–ª—å—Ç–∞—Ç —Ç–µ–º —É –∫–æ–≥–æ –æ–Ω–æ –æ—Ç–∫—Ä—ã—Ç–æ
rowMembers = 9;
while(sheetMembers.getRange(rowMembers,4).getValue()>0) { //—à–ª–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏—è –≤—Å–µ–º —É –∫–æ–≥–æ –æ—Ç–∫—Ä—ã—Ç–æ –º–µ–Ω—é –†–µ–∑—É–ª—å—Ç–∞—Ç 
  messageId = sheetMembers.getRange(rowMembers,28).getValue();
  if(messageId>0 ) fromId = sheetMembers.getRange(rowMembers,2).getValue(); rez(0);
  rowMembers = rowMembers + 1;                        
}    
}

function sendMG(d,row){ //–≤—ã–≤–æ–¥–∏–º –∞–ª—å–±–æ–º —Ñ–æ—Ç–æ–≥—Ä–∞—Ñ–∏–π –¥–æ–∫—É–º–µ–Ω—Ç–æ–≤.
  var m = [];
  for(var i=1; i<11; i++) {
    var cell = sheetDoki.getRange(row, 10*d+i).getValue();
    if(cell!=0) m.push(cell.split(' '));}
  if(d==3) {
    m.length = 0;
    for(var j=0; j<4; j++) {
      for(var i=1; i<11; i++){
        var cell = sheetDoki.getRange(row+j, 10*d+i).getValue();
        if(cell!=0) m.push(cell.split(' '));}}        
  }
  if(m.length>1) {m.sort(function(a, b){return b[0] - a[0]}); //—Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ —É–±—ã–≤–∞–Ω–∏—é –ø–æ –ø–µ—Ä–≤–æ–º—É –ø–æ–ª—é - –Ω–æ–º–µ—Ä—É –∞–ª—å–±–æ–º–∞
    for(var i=1; i<m.length; i++) {if(m[i][0]<m[i-1][0]) {m.length=i; break;}} //–æ–±—Ä–µ–∑–∞–µ–º –≤—Å–µ –±–æ–ª–µ–µ —Ä–∞–Ω–Ω–∏–µ –∞–ª—å–±–æ–º—ã
    m.sort(function(a, b){return a[1] - b[1]});} //—Å–æ—Ä—Ç–∏—Ä–æ–≤–∫–∞ –ø–æ –≤–æ–∑—Ä–∞—Å—Ç–∞–Ω–∏—é –ø–æ –≤—Ç–æ—Ä–æ–º—É –ø–æ–ª—é - –Ω–æ–º–µ—Ä—É —Å–æ–æ–±—â–µ–Ω–∏—è
   
  var med = [];
  var n = sheetMembers.getRange(1*row,4).getValue()+' '+sheetMembers.getRange(row,3).getValue(); //–∫–æ–º–º–µ–Ω—Ç–∞—Ä–∏–π –∫ –≤—ã–≤–æ–¥–∏–º—ã–º —Ñ–æ—Ç–∫–∞–º - –Ω–æ–º–µ—Ä —É–∏–∫ –∏ —Ñ–∏–æ
  if(d==3) n = sheetMembers.getRange(1*row,4).getValue()+'\n'+sheetMembers.getRange(row,6).getValue();
  if(m.length>0){
    for(var i=0; i<m.length; i++) 
      if(i==0) med.push({type:'photo',media:m[i][2],caption:n}); //–≤ –≤—ã–≤–æ–¥–∏–º–æ–º –∞–ª—å–±–æ–º–µ –∫–æ–º–º–µ–Ω—Ç–∏—Ä—É–µ–º —Ç–æ–ª—å–∫–æ –ø–µ—Ä–≤—É—é —Ñ–æ—Ç–∫—É
        else med.push({type:'photo',media:m[i][2]});
  //med = [{type:'photo',media:'AgADAgADe6oxG6WLKUrLDqFzIu5ZrDb2tw8ABAEAAwIAA3gAA8nLAQABFgQ',caption:'aCsd'},{type:'photo',media:'AgADAgADy6sxGyMDKUr93Yp-UHJMtvDeug8ABAEAAwIAA3kAA-FYAAIWBA',caption:'asd'}];
  var data = {
    method: "post",
    payload: {method: "sendMediaGroup",chat_id: String(fromId),media: JSON.stringify(med),}
  }
  UrlFetchApp.fetch('https://api.telegram.org/bot' + API_TOKEN + '/', data);
  log(fromId,fullName,name,rowMembers,cbText,botUpdate);
  }
  else {
    if(d!=3) answercb(sheetMembers.getRange(1*row,4).getValue()+'  '+sheetMembers.getRange(1*row,13).getValue()+'\n'+sheetMembers.getRange(1*row,3).getValue()+'\n\n–ù–∏—á–µ–≥–æ –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω–æ',cbId);
      else answercb(sheetMembers.getRange(1*row,4).getValue()+'\n\n–§–æ—Ç–æ–≥—Ä–∞—Ñ–∏–∏ —Å —É—á–∞—Å—Ç–∫–∞ –Ω–µ –∑–∞–≥—Ä—É–∂–µ–Ω—ã',cbId);
  }
}


