      var med = sheetMembers.getRange(j,24).getValue(); med = med.slice(med.indexOf("1")+1);
      dBut.push([{text:med+uik,callback_data:'22u'+j}]); //–¥–æ–±–∞–≤–ª—è–µ–º —Å—Ç–æ–ª–±–µ—Ü –Ω–æ–º–µ—Ä–æ–≤ –∫–æ–º–∏—Å—Å–∏–π
